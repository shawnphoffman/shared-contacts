{
	"name": "shared-contacts",
	"version": "0.8.1",
	"description": "Self-hostable CardDAV server for managing shared contacts with a modern web-based management interface",
	"private": true,
	"engines": {
		"node": ">=20.0.0",
		"npm": ">=9.0.0"
	},
	"scripts": {
		"release": "release-it",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"lint": "npm --prefix ui run lint && npm --prefix sync-service run lint",
		"lint:fix": "npm --prefix ui run lint -- --fix && npm --prefix sync-service run lint -- --fix",
		"lint:ui": "npm --prefix ui run lint",
		"lint:sync": "npm --prefix sync-service run lint"
	},
	"keywords": [
		"carddav",
		"contacts",
		"radicale",
		"self-hosted",
		"sync"
	],
	"author": "Shawn Hoffman",
	"repository": {
		"type": "git",
		"url": "https://github.com/shawnphoffman/shared-contacts.git"
	},
	"bugs": {
		"url": "https://github.com/shawnphoffman/shared-contacts/issues"
	},
	"homepage": "https://github.com/shawnphoffman/shared-contacts#readme",
	"devDependencies": {
		"@release-it/conventional-changelog": "^10.0.4",
		"auto-changelog": "^2.5.0",
		"prettier": "^3.5.3",
		"release-it": "^19.2.3"
	}
}
